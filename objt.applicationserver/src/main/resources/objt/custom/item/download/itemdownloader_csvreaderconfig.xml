<?xml version="1.0"?>
<readerconfigs>
  <readerconfig id="objt.custom.common.general.itemmgt.download.ItemDownloader" class="dce.ei.reader.CSVReaderConfig" usecolumnheader="yes">
    <field id="name"          columnname="ITEM_NAME"           type="string"  trim="YES" required="YES"/>
    <field id="deleted"       columnname="ITEM_DELETE"         type="boolean" trim="YES" format="D"/>
    <field id="description"   columnname="ITEM_DESCRIPTION"    type="string"  trim="YES"/>
    <field id="id"            columnname="ITEM_ID"             type="string"  trim="YES"/>
    <field id="eanCode"       columnname="ITEM_EANCODE"        type="string"  trim="YES"/>
    <field id="itemGroupName"           columnname="ITEM_ITEMGROUP_NAME"           type="string"  trim="YES"/>
    <field id="putAwayRuleGroupName"    columnname="ITEM_PUTAWAYRULEGROUP_NAME"    type="string"  trim="YES"/>
    <field id="relocationRuleGroupName" columnname="ITEM_RELOCATIONRULEGROUP_NAME" type="string"  trim="YES"/>
    <field id="crossDockRuleGroupName"  columnname="ITEM_CROSSDOCKRULEGROUP_NAME"  type="string"  trim="YES"/>
    <field id="pickRuleGroupName"       columnname="ITEM_PICKRULEGROUP_NAME"       type="string"  trim="YES"/>
    <field id="replenishRuleGroupName"  columnname="ITEM_REPLENISHRULEGROUP_NAME"  type="string"  trim="YES"/>
    <field id="inspectionGroupName"     columnname="ITEM_INSPECTIONGROUP_NAME"     type="string"  trim="YES"/>
    <field id="baseUomName"   columnname="ITEM_UOM_NAME"       type="string"  trim="YES" required="YES"/>

    <!-- the dimensioned parameter fields are optional, but can also be specified empty to force removal of a previously assigned value. -->
    <field id="weight"        columnname="ITEM_WEIGHT_VALUE"    type="double" trim="YES"/>
    <field id="weightUomName" columnname="ITEM_WEIGHT_UOM_NAME" type="string" trim="YES"/>
    <field id="width"         columnname="ITEM_WIDTH_VALUE"     type="double" trim="YES"/>
    <field id="widthUomName"  columnname="ITEM_WIDTH_UOM_NAME"  type="string" trim="YES"/>
    <field id="depth"         columnname="ITEM_DEPTH_VALUE"     type="double" trim="YES"/>
    <field id="depthUomName"  columnname="ITEM_DEPTH_UOM_NAME"  type="string" trim="YES"/>
    <field id="height"        columnname="ITEM_HEIGHT_VALUE"    type="double" trim="YES"/>
    <field id="heightUomName" columnname="ITEM_HEIGHT_UOM_NAME" type="string" trim="YES"/>
    <field id="volume"        columnname="ITEM_VOLUME_VALUE"    type="double" trim="YES"/>
    <field id="volumeUomName" columnname="ITEM_VOLUME_UOM_NAME" type="string" trim="YES"/>
    <field id="cubics"        columnname="ITEM_CUBICS"          type="double" trim="YES" />

    <field id="type"                        columnname="ITEM_TYPE"                       type="integer" trim="YES"/>
    <field id="category"                    columnname="ITEM_CATEGORY"                   type="string"  trim="YES"/>
    <field id="lotControl"                  columnname="ITEM_LOTCONTROL"                 type="integer" trim="YES"/>
    <field id="lotControlled"               columnname="ITEM_ISLOTCONTROLLED"            type="boolean" trim="YES" format="true,waar,vrai"/>
    <field id="subLotControlled"            columnname="ITEM_ISSUBLOTCONTROLLED"         type="boolean" trim="YES" format="true,waar,vrai"/>
    <field id="lotFormat"                   columnname="ITEM_LOTFORMAT"                  type="string"  trim="YES"/>
    <field id="shelfLife"                   columnname="ITEM_SHELFLIFE"                  type="integer" trim="YES"/>
    <field id="shelfLifeUnit"               columnname="ITEM_SHELFLIFEUNIT"              type="integer" trim="YES"/>
    <field id="shelfLifePrecision"          columnname="ITEM_SHELFLIFEPRECISION"         type="integer" trim="YES"/>
    <field id="inventoryForecastEnabled"    columnname="ITEM_ISINVENTORYFORECASTENABLED" type="boolean" trim="YES" format="true,waar,vrai"/>
    <field id="stockCheckLevel"             columnname="ITEM_STOCKCHECKLEVEL"            type="double"  trim="YES"/>
    <field id="abcLpn"                      columnname="ITEM_ABC_LPN"                    type="string"  trim="YES"/>
    <field id="abcSku"                      columnname="ITEM_ABC_SKU"                    type="string"  trim="YES"/>
    <field id="packConfig"                  columnname="ITEM_PACKCONFIG"                 type="integer" trim="YES"/>

    <!-- the following fields are optional, but can also be specified empty to force removal of a previously assigned value. -->
    <field id="qualityControlStatusID" columnname="QUALITYCONTROLSTATUS_ID" type="string"  trim="YES" />
    <field id="serialControl"          columnname="ITEM_SERIALCONTROL"      type="integer" trim="YES" />
    <field id="serialTracing"          columnname="ITEM_SERIALTRACING"      type="integer" trim="YES" />
    <field id="serialFormat"           columnname="ITEM_SERIALFORMAT"       type="string"  trim="YES" />
    <field id="serialPattern"          columnname="ITEM_SERIALPATTERN"      type="string"  trim="YES" />

    <field id="usrTxt1" columnname="ITEM_USRTXT1" type="string"   trim="YES"/>
    <field id="usrTxt2" columnname="ITEM_USRTXT2" type="string"   trim="YES"/>
    <field id="usrTxt3" columnname="ITEM_USRTXT3" type="string"   trim="YES"/>
    <field id="usrTxt4" columnname="ITEM_USRTXT4" type="string"   trim="YES"/>
    <field id="usrTxt5" columnname="ITEM_USRTXT5" type="string"   trim="YES"/>
    <field id="usrDts1" columnname="ITEM_USRDTS1" type="date"     trim="YES" format="dd/MM/yyyy"/>
    <field id="usrDts2" columnname="ITEM_USRDTS2" type="date"     trim="YES" format="dd/MM/yyyy"/>
    <field id="usrDts3" columnname="ITEM_USRDTS3" type="date"     trim="YES" format="dd/MM/yyyy"/>
    <field id="usrDts4" columnname="ITEM_USRDTS4" type="date"     trim="YES" format="dd/MM/yyyy"/>
    <field id="usrDts5" columnname="ITEM_USRDTS5" type="date"     trim="YES" format="dd/MM/yyyy"/>
    <field id="usrFlg1" columnname="ITEM_USRFLG1" type="boolean"  trim="YES" format="true,waar,vrai"/>
    <field id="usrFlg2" columnname="ITEM_USRFLG2" type="boolean"  trim="YES" format="true,waar,vrai"/>
    <field id="usrFlg3" columnname="ITEM_USRFLG3" type="boolean"  trim="YES" format="true,waar,vrai"/>
    <field id="usrFlg4" columnname="ITEM_USRFLG4" type="boolean"  trim="YES" format="true,waar,vrai"/>
    <field id="usrFlg5" columnname="ITEM_USRFLG5" type="boolean"  trim="YES" format="true,waar,vrai"/>
    <field id="usrNum1" columnname="ITEM_USRNUM1" type="double"   trim="YES"/>
    <field id="usrNum2" columnname="ITEM_USRNUM2" type="double"   trim="YES"/>
    <field id="usrNum3" columnname="ITEM_USRNUM3" type="double"   trim="YES"/>
    <field id="usrNum4" columnname="ITEM_USRNUM4" type="double"   trim="YES"/>
    <field id="usrNum5" columnname="ITEM_USRNUM5" type="double"   trim="YES"/>

  </readerconfig>
</readerconfigs>
