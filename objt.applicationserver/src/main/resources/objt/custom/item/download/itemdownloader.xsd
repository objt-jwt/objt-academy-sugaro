              <xs:element name="usrdts2" type="objt:dateordatetimeorempty" minOccurs="0"/>
              <xs:element name="usrdts3" type="objt:dateordatetimeorempty" minOccurs="0"/>
              <xs:element name="usrdts4" type="objt:dateordatetimeorempty" minOccurs="0"/>
              <xs:element name="usrdts5" type="objt:dateordatetimeorempty" minOccurs="0"/>
              <xs:element name="usrflg1" type="objt:booleanorempty" minOccurs="0"/>
              <xs:element name="usrflg2" type="objt:booleanorempty" minOccurs="0"/>
              <xs:element name="usrflg3" type="objt:booleanorempty" minOccurs="0"/>
              <xs:element name="usrflg4" type="objt:booleanorempty" minOccurs="0"/>
              <xs:element name="usrflg5" type="objt:booleanorempty" minOccurs="0"/>
              <xs:element name="usrnum2" type="objt:doubleorempty" minOccurs="0"/>
              <xs:element name="usrnum3" type="objt:doubleorempty" minOccurs="0"/>
              <xs:element name="usrnum1" type="objt:doubleorempty" minOccurs="0"/>
              <xs:element name="usrnum4" type="objt:doubleorempty" minOccurs="0"/>
