              <xs:element name="isinventoryforecastenabled" type="objt:booleanorempty" minOccurs="0"/>
              <xs:element name="stockchecklevel" type="objt:doubleorempty" minOccurs="0"/>
              <xs:element name="qualitycontrolstatusid" type="objt:string32" minOccurs="0"/>
              <xs:element name="usrtxt1" type="objt:string255" minOccurs="0"/>
              <xs:element name="usrtxt2" type="objt:string255" minOccurs="0"/>
              <xs:element name="usrtxt3" type="objt:string255" minOccurs="0"/>
              <xs:element name="usrtxt4" type="objt:string255" minOccurs="0"/>
              <xs:element name="usrtxt5" type="objt:string255" minOccurs="0"/>
