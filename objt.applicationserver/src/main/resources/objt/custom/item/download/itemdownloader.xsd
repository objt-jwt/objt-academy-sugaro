      <xs:sequence>
        <xs:element name="item" maxOccurs="unbounded">
          <xs:complexType>
            <xs:all>
              <xs:element name="name" type="objt:string64"/>
              <xs:element name="delete" type="objt:booleanorempty" minOccurs="0"/>
              <xs:element name="description" type="objt:string255" minOccurs="0"/>
              <xs:element name="id" type="objt:string32" minOccurs="0"/>
              <xs:element name="eancode" type="objt:string16" minOccurs="0"/>
              <xs:element name="itemgroupname" type="objt:string64" minOccurs="0"/>
              <xs:element name="putawayrulegroupname" type="objt:string64" minOccurs="0"/>
              <xs:element name="relocationrulegroupname" type="objt:string64" minOccurs="0"/>
              <xs:element name="crossdockrulegroupname" type="objt:string64" minOccurs="0"/>
              <xs:element name="pickrulegroupname" type="objt:string64" minOccurs="0"/>
              <xs:element name="replenishrulegroupname" type="objt:string64" minOccurs="0"/>
              <xs:element name="inspectiongroupname" type="objt:string64" minOccurs="0"/>
              <xs:element name="uomname" type="objt:string32"/> <!-- required field-->
              <xs:element name="weight" type="objt:doubleorempty"  minOccurs="0"/>
              <xs:element name="weightuomname" type="xs:string" minOccurs="0"/>
              <xs:element name="width" type="objt:doubleorempty" minOccurs="0"/>
              <xs:element name="widthuomname" type="xs:string" minOccurs="0"/>
              <xs:element name="depth" type="objt:doubleorempty" minOccurs="0"/>
              <xs:element name="depthuomname" type="xs:string" minOccurs="0"/>
              <xs:element name="height" type="objt:doubleorempty" minOccurs="0"/>
              <xs:element name="heightuomname" type="xs:string" minOccurs="0"/>
              <xs:element name="volume" type="objt:doubleorempty" minOccurs="0"/>
              <xs:element name="volumeuomname" type="xs:string" minOccurs="0"/>
              <xs:element name="cubics" type="objt:doubleorempty" minOccurs="0"/>
              <xs:element name="type" type="objt:integerorempty" minOccurs="0"/>
              <xs:element name="category" type="objt:string64" minOccurs="0"/>
              <xs:element name="lotcontrol" type="objt:integerorempty" minOccurs="0"/>
              <xs:element name="islotcontrolled" type="objt:booleanorempty" minOccurs="0"/>
              <xs:element name="issublotcontrolled" type="objt:booleanorempty" minOccurs="0"/>
              <xs:element name="lotformat" type="xs:string" minOccurs="0"/>
