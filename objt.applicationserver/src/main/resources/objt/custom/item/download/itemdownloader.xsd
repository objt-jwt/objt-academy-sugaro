              <xs:element name="serialformat"   type="objt:string64" minOccurs="0"/>
              <xs:element name="serialpattern"  type="objt:string64" minOccurs="0"/>
              <xs:element name="packconfig" type="objt:integerorempty" minOccurs="0"/>
            </xs:all>
          </xs:complexType>
        </xs:element>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
</xs:schema>